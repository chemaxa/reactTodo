{
  "name": "react-todo-example",
  "version": "0.0.1",
  "description": "My react todo example app",
  "main": "app.js",
  "scripts": {
    "test": "test",
    "dev": "node devserver",
    "build": "webpack -p --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chemaxa/reactTodo.git"
  },
  "keywords": [
    "react",
    "todo",
    "app"
  ],
  "author": "Maxim Shkarbalyuk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chemaxa/reactTodo/issues"
  },
  "homepage": "https://github.com/chemaxa/reactTodo#readme",
  "dependencies": {
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0",
    "koa": "^1.2.0",
    "kcors": "^1.2.1",
    "koa-body": "^1.4.0",
    "koa-logger": "^1.3.0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "mongoose": "^4.5.3"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "eslint": "^2.10.1",
    "eslint-plugin-react": "^5.1.1",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "strict": [
        2,
        "never"
      ],
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/react-in-jsx-scope": 2
    },
    "plugins": [
      "react"
    ]
  }
}
